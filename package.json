		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/vite": "^4.1.8",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.2",
		"gh-pages": "^6.3.0",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"svelte": "^5.33.19",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.0",
		"vite": "^6.3.5"
